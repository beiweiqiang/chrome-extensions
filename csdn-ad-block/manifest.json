{
  "manifest_version": 2,
  "name": "csdn ad block",
  "version": "0.0.1",
  "description": "csdn 广告屏蔽, 自动点击更多",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://blog.csdn.net/*"],
      "js": ["js/ad_block.js"]
    }
  ]
}